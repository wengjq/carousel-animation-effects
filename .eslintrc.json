{
  "extends": "airbnb-base",
  "rules": {
    "no-var": [
      0
    ],
    "arrow-body-style": [0],
    "strict": [
      0
    ],
    "no-console": [
      0
    ],
    "func-names": [
      0
    ],
    "space-before-function-paren": [
      0
    ],
    "no-param-reassign": [
      0
    ],
    "vars-on-top": [
      0
    ],
    "linebreak-style": ["error", "windows"],
    "no-restricted-syntax": ["error", "WithStatement"]
  }
}